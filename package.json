  "homepage": "https://pfw360.powerobjects.com",
  "version": "1.0.0-GitVersion",
  "license": "MIT",
  "keywords": [
    "component",
    "d365",
    "fluent-ui",
    "javascript",
    "library",
    "pcf",
    "react",
    "storybook",
    "typescript",
    "unified-interface"
